FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    Cyl_top
    {
        type            zeroGradient;
    }

    Cyl_mdl
    {
        type            zeroGradient;
    }

    Cyl_btm
    {
        type            zeroGradient;
    }
}

